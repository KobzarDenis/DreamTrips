{
  "name": "DreamTrips Funnel",
  "description": "App for making a super funnel",
  "addons": [
    "heroku-postgresql",
    {
      "as": "DATABASE_URL",
      "plan": "hobby-basic",
      "options": {
        "name": "dreamtrips-prod-postgres"
      }
    },
    "heroku-redis",
    {
      "as": "REDIS_URL",
      "plan": "hobby-dev",
      "options": {
        "name": "dreamtrips-prod-redis"
      }
    }
  ]
}
